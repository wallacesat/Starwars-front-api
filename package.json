{
  "name": "swapiweb",
  "version": "0.1.0",
  "private": true,
  "proxy": "http://localhost:3001/",
  "scripts": {
    "start": "npm-run-all -p watch:back start:front",
    "watch:back": "nodemon --exec \"npm run start:back\" api --ignore api/fakeData/db.json",
    "prestart:back": "cross-env NODE_ENV=development npx babel-node ./api/generateFakeData.js --presets @babel/preset-env",
    "start:back": "cross-env NODE_ENV=development npx babel-node ./api/index.js --presets @babel/preset-env",
    "start:front": "node scripts/start.js",
    "build": "node scripts/build.js",
    "test": "node scripts/test.js --verbose"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "dependencies": {
    "@babel/core": "^7.2.2",
    "@material-ui/core": "^3.9.2",
    "@material-ui/icons": "^3.0.2",
    "@material-ui/styles": "^3.0.0-alpha.10",
    "@svgr/webpack": "4.1.0",
    "axios": "^0.19.0",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "9.0.0",
    "babel-jest": "23.6.0",
    "babel-loader": "8.0.5",
    "babel-plugin-named-asset-import": "^0.3.1",
    "babel-preset-react-app": "^7.0.2",
    "bfj": "6.1.1",
    "bootstrap": "^4.3.1",
    "browserlist": "^1.0.1",
    "case-sensitive-paths-webpack-plugin": "2.2.0",
    "css-loader": "1.0.0",
    "dotenv": "6.0.0",
    "dotenv-expand": "4.2.0",
    "eslint": "5.12.0",
    "eslint-config-react-app": "^3.0.8",
    "eslint-loader": "2.1.1",
    "eslint-plugin-flowtype": "2.50.1",
    "eslint-plugin-import": "2.14.0",
    "eslint-plugin-jsx-a11y": "6.1.2",
    "eslint-plugin-react": "7.12.4",
    "file-loader": "2.0.0",
    "fs-extra": "7.0.1",
    "html-webpack-plugin": "4.0.0-alpha.2",
    "identity-obj-proxy": "3.0.0",
    "jest": "23.6.0",
    "jest-pnp-resolver": "1.0.2",
    "jest-resolve": "23.6.0",
    "jest-watch-typeahead": "^0.2.1",
    "js-yaml": "3.13.1",
    "lodash": "4.17.13",
    "lodash.template": "4.5.0",
    "mini-css-extract-plugin": "0.5.0",
    "mixin-deep": "1.3.2",
    "nock": "^10.0.6",
    "optimize-css-assets-webpack-plugin": "5.0.1",
    "pnp-webpack-plugin": "1.2.1",
    "postcss-flexbugs-fixes": "4.1.0",
    "postcss-loader": "3.0.0",
    "postcss-preset-env": "6.5.0",
    "postcss-safe-parser": "4.0.1",
    "react": "^16.8.2",
    "react-app-polyfill": "^0.2.2",
    "react-dev-utils": "^11.0.4",
    "react-dom": "^16.8.2",
    "react-router-dom": "^4.3.1",
    "reactstrap": "^7.1.0",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.3.0",
    "resolve": "1.10.0",
    "route-auth": "^2.0.1",
    "sass-loader": "7.1.0",
    "set-value": "2.0.1",
    "style-loader": "0.23.1",
    "terser-webpack-plugin": "1.2.2",
    "url-loader": "1.1.2",
    "webpack": "4.28.3",
    "webpack-dev-server": "3.1.14",
    "webpack-manifest-plugin": "2.0.4",
    "workbox-webpack-plugin": "3.6.3"
  },
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/node": "^7.2.2",
    "@babel/preset-env": "^7.4.2",
    "body-parser": "^1.18.3",
    "chalk": "^2.4.2",
    "chance": "^1.0.18",
    "cross-env": "^6.0.0",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.11.2",
    "eslint-utils": "^1.4.2",
    "express": "^4.16.4",
    "istanbul": "^0.4.5",
    "jest-html-reporters": "^1.1.3",
    "json-schema-faker": "^0.5.0-rc16",
    "nodemon": "^1.18.10",
    "npm-run-all": "^4.1.5",
    "react-redux": "^6.0.1",
    "redux": "^4.0.1",
    "redux-mock-store": "^1.5.3"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/**/*.d.ts"
    ],
    "resolver": "jest-pnp-resolver",
    "setupFiles": [
      "react-app-polyfill/jsdom",
      "<rootDir>/src/__tests__/setup.js"
    ],
    "testMatch": [
      "<rootDir>/src/__tests__/**/?(*.)(spec|test).js?(x)"
    ],
    "testEnvironment": "jsdom",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx|ts|tsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|ts|tsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|ts|tsx)$",
      "^.+\\.module\\.(css|sass|scss)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web",
      "^.+\\.module\\.(css|sass|scss)$": "identity-obj-proxy"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "web.ts",
      "ts",
      "web.tsx",
      "tsx",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ],
    "reporters": [
      "default",
      "jest-html-reporters"
    ],
    "watchPlugins": [
      "<rootDir>/node_modules/jest-watch-typeahead/filename.js",
      "<rootDir>/node_modules/jest-watch-typeahead/testname.js"
    ],
    "watchPathIgnorePatterns": [
      ".*jest-stare.*\\.js"
    ]
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  }
}
